{
    "name": "akw-app",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "prepare": "husky",
        "dev": "vite",
        "build": "tsc -b && vite build",
        "static:check": "eslint . && prettier --log-level silent --check --ignore-unknown .",
        "static:fix": "eslint . --fix && prettier --log-level silent --write --ignore-unknown .",
        "preview": "vite preview",
        "test": "vitest --config ./vitest.config.ts --run",
        "test:watch": "vitest --config ./vitest.config.ts"
    },
    "dependencies": {
        "@tanstack/react-router": "^1.95.3",
        "husky": "^9.1.7",
        "react": "^18.3.1",
        "react-dom": "^18.3.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.17.0",
        "@tanstack/router-devtools": "^1.95.3",
        "@tanstack/router-plugin": "^1.95.3",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.1.0",
        "@types/react": "^18.3.18",
        "@types/react-dom": "^18.3.5",
        "@vitejs/plugin-react-swc": "^3.5.0",
        "eslint": "^9.17.0",
        "eslint-plugin-react-hooks": "^5.0.0",
        "eslint-plugin-react-refresh": "^0.4.16",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "globals": "^15.14.0",
        "happy-dom": "^16.5.3",
        "prettier": "^3.4.2",
        "typescript": "~5.6.2",
        "typescript-eslint": "^8.18.2",
        "vite": "^6.0.5",
        "vitest": "^2.1.8"
    }
}
